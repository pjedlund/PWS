{
  "name": "PWS",
  "version": "1.0.0",
  "description": "Personal Web Site",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "run-s clean eleventy:prod --print-label",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "del-cli dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/pjedlund/PWS"
  },
  "author": "Per Johan Edlund",
  "license": "Unlicense",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "babel-loader": "^8.2.5",
    "clean-css": "^5.3.0",
    "critical": "^4.0.1",
    "cssesc": "^3.0.0",
    "del-cli": "^3.0.1",
    "eleventy-plugin-svg-sprite": "^1.2.1",
    "focus-trap": "^6.9.3",
    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-toc-done-right": "^4.2.0",
    "memfs": "^3.4.4",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "svg-sprite": "^1.5.0",
    "webpack": "^5.72.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eleventy-plugin-page-assets": "^0.3.0"
  }
}
